<?xml version='1.0' encoding='utf-8'?>
<widget id="com.parasiteinfection.parasiteinfectiongame" version="5.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Parasite Infection</name>
    <description>Sample Apache Cordova App</description>
    <author email="leykrvr@gmail.com" href="http://cordova.io">
        Leykoss
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="mailto:*" />
    <hook src="scripts/RenameInitial.js" type="after_prepare" />
    <allow-intent href="market:*" />
    <icon density="hdpi" src="res/android/mipmap-hdpi/ic_launcher.png" />
    <icon density="mdpi" src="res/android/mipmap-mdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/android/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/android/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="res/android/mipmap-xxxhdpi/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
